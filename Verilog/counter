S_0x562d3c5a4f60 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x562d3c5a50f0 .scope module, "test" "test" 3 1;
 .timescale 0 0;
v0x562d3c5b8c60_0 .var "clk", 0 0;
v0x562d3c5b8d30_0 .var "reset", 0 0;
v0x562d3c5b8e00_0 .net "value", 7 0, v0x562d3c56dec0_0;  1 drivers
S_0x562d3c56bef0 .scope module, "c1" "counter" 3 18, 4 1 0, S_0x562d3c5a50f0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x562d3c56c0d0 .param/l "WIDTH" 0 4 3, +C4<00000000000000000000000000001000>;
v0x562d3c56e120_0 .net "clk", 0 0, v0x562d3c5b8c60_0;  1 drivers
v0x562d3c56dec0_0 .var "out", 7 0;
v0x562d3c5b8b10_0 .net "reset", 0 0, v0x562d3c5b8d30_0;  1 drivers
E_0x562d3c5a41f0 .event posedge, v0x562d3c5b8b10_0, v0x562d3c56e120_0;
    .scope S_0x562d3c56bef0;
T_0 ;
    %wait E_0x562d3c5a41f0;
    %load/vec4 v0x562d3c5b8b10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x562d3c56dec0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562d3c56dec0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x562d3c56dec0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562d3c5a50f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d3c5b8d30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d3c5b8c60_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x562d3c5a50f0;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562d3c5b8d30_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d3c5b8d30_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562d3c5b8d30_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d3c5b8d30_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 10 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x562d3c5a50f0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x562d3c5b8c60_0;
    %nor/r;
    %store/vec4 v0x562d3c5b8c60_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x562d3c5a50f0;
T_4 ;
    %vpi_call/w 3 22 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call/w 3 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x562d3c5a50f0 {0 0 0};
